{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "prisma generate && next build",
  "framework": "nextjs",
  "regions": ["cdg1"],
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "src/app/api/projets/[id]/bons-commande/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "installCommand": "pnpm install",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
